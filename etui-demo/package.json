{
	"name": "etui-demo",
	"version": "0.1.0",
	"description": "Demo application for etui",
	"type": "module",
	"private": true,
	"scripts": {
		"start": "node --enable-source-maps --loader ./loader.mjs app/index.ts",
		"dev": "rollup -c -w",
		"build": "rollup -c",
		"type": "ember-tsc -c",
		"test": "vitest --run --printConsoleTrace --disable-console-intercept --silent=false",
		"test:update": "vitest --run --update"
	},
	"exports": {
		"./*": "./app/*",
		"./tests/*": "./tests/*"
	},
	"dependencies": {
		"@babel/runtime": "^7.23.0",
		"@glimmer/component": "^2.0.0",
		"chalk": "^5.3.0",
		"etui": "link:../etui",
		"ember-load-initializers": "^2.1.2",
		"ember-resolver": "^13.1.1",
		"ember-source": "^6.8.2"
	},
	"devDependencies": {
		"@babel/core": "^7.23.0",
		"@babel/plugin-proposal-class-properties": "^7.18.6",
		"@babel/plugin-proposal-decorators": "^7.23.0",
		"@babel/plugin-transform-runtime": "^7.23.0",
		"@babel/plugin-transform-typescript": "^7.23.0",
		"@ember/test-helpers": "^5.4.1",
		"@embroider/compat": "^4.1.10",
		"@embroider/core": "^4.2.8",
		"@embroider/macros": "^1.13.0",
		"@embroider/vite": "^1.4.2",
		"@glint/ember-tsc": "^1.0.8",
		"@glint/template": "^1.7.3",
		"@glint/tsserver-plugin": "^2.0.8",
		"@rollup/plugin-babel": "^6.0.4",
		"@rollup/plugin-commonjs": "^25.0.7",
		"@rollup/plugin-json": "^6.1.0",
		"@rollup/plugin-node-resolve": "^15.2.3",
		"@types/node": "^20.10.0",
		"babel-plugin-ember-template-compilation": "^2.2.0",
		"content-tag": "^4.0.0",
		"decorator-transforms": "^2.0.0",
		"ember-compatibility-helpers": "^1.2.7",
		"ember-vitest": "^0.2.1",
		"indent-string": "^5.0.0",
		"jimp": "^1.6.0",
		"rollup": "^4.9.0",
		"tsx": "^4.7.0",
		"typescript": "^5.9.3",
		"vite": "^7.2.4",
		"vitest": "^3.2.4",
		"gradient-string": "^3.0.0"
	},
	"ember": {
		"edition": "octane"
	}
}
